@model List<User>

<div id="usersContainer">
    <h3>Users</h3>
    @foreach (var user in Model)
    {
        <div class="item">
            <h4>@user.id</h4>
            <p>@user.name</p>
            <p>@user.username</p>
            <p>@user.email</p>
            <p>@user.phone</p>
            <p>@user.website</p>
        </div>
    }
</div>
